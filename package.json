{
  "name": "pigallery",
  "version": "1.1.13",
  "description": "PiGallery: Photo Gallery and Analysis using TensorFlow/JS and EXIF metadata",
  "main": "server/pigallery.js",
  "scripts": {
    "start": "node server/pigallery.js",
    "install": "node ./setup.js",
    "version": "node server/changelog.js",
    "upgrade": "node ./setup.js",
    "lint": "htmlhint client/*.html && eslint client/*.js server/*.js *.json && stylelint client/* && depcheck --ignores='eslint-plugin-*,auditjs,npm-check-updates,stylelint-config-standard' --ignore-dirs='dist' && auditjs ossi --quiet"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "browserslist": [
    "last 1 Chrome version"
  ],
  "staticFiles": {
    "staticPath": "assets",
    "excludeGlob": [
      "*"
    ]
  },
  "stylelint": {
    "extends": "stylelint-config-standard",
    "rules": {
      "declaration-block-single-line-max-declarations": 50,
      "comment-empty-line-before": "never",
      "block-closing-brace-newline-after": "always-single-line",
      "at-rule-empty-line-before": "never",
      "selector-list-comma-newline-after": "always-multi-line",
      "color-hex-length": "long"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vladmandic/pigallery.git"
  },
  "keywords": [
    "tensorflow",
    "tensorflowjs",
    "tfjs",
    "mobilenet",
    "imagenet",
    "efficientnet",
    "cocossd",
    "darknet",
    "yolo",
    "faceapi",
    "resnet",
    "inception",
    "gallery",
    "metadata"
  ],
  "author": "Vladimir Mandic <mandic00@live.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vladmandic/pigallery/issues"
  },
  "homepage": "https://github.com/vladmandic/pigallery#readme",
  "dependencies": {
    "@tensorflow/tfjs": "^1.7.4",
    "chokidar": "^3.4.1",
    "esbuild": "^0.5.26",
    "exif-parser": "^0.1.12",
    "express": "^5.0.0-alpha.8",
    "express-session": "^1.17.1",
    "face-api.js": "^0.22.2",
    "jpeg-exif": "^1.1.4",
    "marked": "^1.1.1",
    "moment": "^2.27.0",
    "nedb-promises": "^4.0.4",
    "pilogger": "git+https://github.com/vladmandic/pilogger.git",
    "shrink-ray-current": "^4.1.2",
    "simple-git": "^2.15.0"
  },
  "devDependencies": {
    "auditjs": "^4.0.18",
    "depcheck": "^0.9.2",
    "eslint": "^7.5.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-html": "^6.0.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-json": "^2.1.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "htmlhint": "^0.14.1",
    "npm-check-updates": "^6.0.1",
    "stylelint": "^13.6.1",
    "stylelint-config-standard": "^20.0.0"
  }
}
