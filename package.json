{
  "name": "pigallery",
  "version": "0.7.3",
  "description": "PiGallery: Photo Gallery and analysis using TensorFlow/JS and EXIF metadata",
  "main": "server/server.js",
  "scripts": {
    "start": "node server/server.js",
    "upgrade": "npm update; npm install; npm dedupe; npm prune"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "browserslist": [
    "last 1 Chrome version"
  ],
  "staticFiles": {
    "staticPath": "assets",
    "excludeGlob": [
      "*"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vladmandic/pigallery.git"
  },
  "keywords": [
    "tensorflow",
    "tensorflowjs",
    "tfjs",
    "mobilenet",
    "imagenet",
    "cocossd",
    "darknet",
    "yolo",
    "faceapi",
    "resnet",
    "inception",
    "gallery",
    "metadata"
  ],
  "author": "Vladimir Mandic <mandic00@live.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vladmandic/pigallery/issues"
  },
  "homepage": "https://github.com/vladmandic/pigallery#readme",
  "dependencies": {
    "@popperjs/core": "^2.4.0",
    "@tensorflow/tfjs": "^1.7.4",
    "@tensorflow/tfjs-backend-cpu": "^2.0.0",
    "@tensorflow/tfjs-backend-wasm": "^2.0.0",
    "@tensorflow/tfjs-backend-webgl": "^2.0.0",
    "compression": "^1.7.4",
    "eslint-config-airbnb-base": "^14.1.0",
    "exif-parser": "^0.1.12",
    "express": "^5.0.0-alpha.8",
    "express-session": "^1.17.1",
    "face-api.js": "^0.22.2",
    "jquery": "^3.5.1",
    "marked": "^1.1.0",
    "moment": "^2.26.0",
    "nedb-promises": "^4.0.3",
    "node-fetch": "^3.0.0-beta.6-exportfix",
    "nsfwjs": "^2.2.0",
    "oboe": "^2.1.5",
    "parcel-bundler": "^1.12.4",
    "pilogger": "git+https://github.com/vladmandic/pilogger.git"
  }
}
