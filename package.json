{
  "name": "pigallery",
  "version": "0.8.4",
  "description": "PiGallery: Photo Gallery and analysis using TensorFlow/JS and EXIF metadata",
  "main": "server/server.js",
  "scripts": {
    "start": "node server/server.js",
    "upgrade": "npm update; npm install; npm dedupe; npm prune",
    "lint": "htmlhint client/*.html && eslint client/*.js server/*.js *.json && depcheck --ignores='eslint-plugin-*,auditjs' --ignore-dirs='dist' && auditjs ossi --quiet"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "browserslist": [
    "last 1 Chrome version"
  ],
  "staticFiles": {
    "staticPath": "assets",
    "excludeGlob": [
      "*"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vladmandic/pigallery.git"
  },
  "keywords": [
    "tensorflow",
    "tensorflowjs",
    "tfjs",
    "mobilenet",
    "imagenet",
    "cocossd",
    "darknet",
    "yolo",
    "faceapi",
    "resnet",
    "inception",
    "gallery",
    "metadata"
  ],
  "author": "Vladimir Mandic <mandic00@live.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vladmandic/pigallery/issues"
  },
  "homepage": "https://github.com/vladmandic/pigallery#readme",
  "dependencies": {
    "@popperjs/core": "^2.4.0",
    "@tensorflow/tfjs": "^1.7.4",
    "compression": "^1.7.4",
    "exif-parser": "^0.1.12",
    "express": "^5.0.0-alpha.8",
    "express-session": "^1.17.1",
    "face-api.js": "^0.22.2",
    "marked": "^1.1.0",
    "moment": "^2.26.0",
    "nedb-promises": "^4.0.3",
    "oboe": "^2.1.5",
    "parcel-bundler": "^1.12.4",
    "pilogger": "git+https://github.com/vladmandic/pilogger.git"
  },
  "devDependencies": {
    "auditjs": "^4.0.18",
    "depcheck": "^0.9.2",
    "eslint": "^7.2.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-html": "^6.0.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-json": "^2.1.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "htmlhint": "^0.14.0",
    "npm-check-updates": "^6.0.1"
  }
}
