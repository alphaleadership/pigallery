{
  "name": "pigallery",
  "version": "1.3.1",
  "description": "PiGallery: Photo Gallery and Analysis using TensorFlow/JS and EXIF metadata",
  "main": "server/pigallery.js",
  "scripts": {
    "start": "node server/pigallery.js",
    "version": "node server/changelog.js",
    "setup": "node ./setup.js",
    "lint": "htmlhint client/*.html && eslint client/*.js server/*.js *.json && && depcheck --ignores='eslint-plugin-*,npm-check-updates,stylelint-config-standard' --ignore-dirs='dist'"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "browserslist": [
    "last 1 Chrome version"
  ],
  "staticFiles": {
    "staticPath": "assets",
    "excludeGlob": [
      "*"
    ]
  },
  "stylelint": {
    "extends": "stylelint-config-standard",
    "rules": {
      "declaration-block-single-line-max-declarations": 50,
      "comment-empty-line-before": "never",
      "block-closing-brace-newline-after": "always-single-line",
      "at-rule-empty-line-before": "never",
      "selector-list-comma-newline-after": "always-multi-line",
      "color-hex-length": "long"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vladmandic/pigallery.git"
  },
  "keywords": [
    "tensorflow",
    "tensorflowjs",
    "tfjs",
    "mobilenet",
    "imagenet",
    "efficientnet",
    "cocossd",
    "darknet",
    "yolo",
    "faceapi",
    "resnet",
    "inception",
    "gallery",
    "metadata"
  ],
  "author": "Vladimir Mandic <mandic00@live.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vladmandic/pigallery/issues"
  },
  "homepage": "https://github.com/vladmandic/pigallery#readme",
  "dependencies": {
    "@tensorflow/tfjs": "^1.7.4",
    "@vladmandic/face-api": "^0.3.2",
    "@vladmandic/pilogger": "^0.1.11",
    "chokidar": "^3.4.2",
    "esbuild": "^0.6.26",
    "exif-parser": "^0.1.12",
    "express": "^5.0.0-alpha.8",
    "express-session": "^1.17.1",
    "jpeg-exif": "^1.1.4",
    "marked": "^1.1.1",
    "moment": "^2.27.0",
    "nedb-promises": "^4.0.4",
    "session-file-store": "^1.4.0",
    "simple-git": "^2.18.0"
  },
  "devDependencies": {
    "depcheck": "^1.2.0",
    "eslint": "^7.7.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-html": "^6.0.3",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-json": "^2.1.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "htmlhint": "^1.0.0-beta.1",
    "npm-check-updates": "^8.0.0-alpha.1"
  }
}
