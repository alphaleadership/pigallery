{
  "name": "@vladmandic/pigallery",
  "version": "2.2.8",
  "description": "PiGallery: Photo Gallery and Analysis using TensorFlow/JS and EXIF metadata",
  "main": "server/pigallery.js",
  "sideEffects": false,
  "scripts": {
    "start": "node --unhandled-rejections=strict --pending-deprecation --trace-deprecation server/pigallery.js",
    "version": "node server/changelog.js",
    "setup": "node ./setup.js",
    "update": "node ./setup.js",
    "lint": "eslint ."
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "browserslist": [
    "last 1 Chrome version"
  ],
  "staticFiles": {
    "staticPath": "assets",
    "excludeGlob": [
      "*"
    ]
  },
  "stylelint": {
    "extends": "stylelint-config-standard",
    "rules": {
      "declaration-block-single-line-max-declarations": 50,
      "comment-empty-line-before": "never",
      "block-closing-brace-newline-after": "always-single-line",
      "at-rule-empty-line-before": "never",
      "selector-list-comma-newline-after": "always-multi-line",
      "color-hex-length": "long"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vladmandic/pigallery.git"
  },
  "keywords": [
    "tensorflow",
    "tensorflowjs",
    "tfjs",
    "mobilenet",
    "imagenet",
    "efficientnet",
    "cocossd",
    "darknet",
    "yolo",
    "faceapi",
    "resnet",
    "inception",
    "gallery",
    "metadata"
  ],
  "author": "Vladimir Mandic <mandic00@live.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vladmandic/pigallery/issues"
  },
  "homepage": "https://github.com/vladmandic/pigallery#readme",
  "dependencies": {
    "@tensorflow/tfjs": "^3.0.0",
    "@tensorflow/tfjs-backend-wasm": "^3.0.0",
    "@vladmandic/face-api": "file:../face-api",
    "@vladmandic/human": "file:../human",
    "@vladmandic/pilogger": "file:../pilogger",
    "buffer": "^6.0.3",
    "chokidar": "^3.5.1",
    "clean-css": "^5.0.0",
    "esbuild": "^0.8.36",
    "exif-parser": "^0.1.12",
    "express": "^5.0.0-alpha.8",
    "express-session": "^1.17.1",
    "jpeg-exif": "^1.1.4",
    "jquery": "^3.5.1",
    "moment": "^2.29.1",
    "nedb-promises": "^4.1.1",
    "node-fetch": "^2.6.1",
    "session-file-store": "^1.5.0",
    "simple-git": "^2.31.0"
  },
  "devDependencies": {
    "eslint": "^7.18.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-html": "^6.1.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-json": "^2.1.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "npm-check-updates": "^11.1.1"
  }
}
